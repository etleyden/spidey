import sqlite3
from datetime import datetime
import pandas as pd

print(datetime.now())

db = sqlite3.connect("db/spidey_db.db")
cursor = db.cursor()
cursor.row_factory = lambda cursor, row: {row[0]}
cursor.execute("SELECT page_id FROM pages")
hashed_urls = pd.DataFrame.from_records(cursor.fetchall())

print(hashed_urls)
